{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
//    "airbnb",
//    "eslint:recommended",
//     "react-app"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0,
        "no-tabs": 0,
        "no-console": 0,
        "no-alert": 0,
        "consistent-return": 0,
        "react/jsx-no-bind": 0,
        "jsx-a11y/label-has-associated-control": 0,
        "object-shorthand": 0,
        "no-plusplus": 0,
        "react/jsx-filename-extension": 0,
        "comma-dangle": 0,
        "arrow-parens": 0,
        "object-curly-newline": 0
    }
}